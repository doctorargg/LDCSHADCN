# Copy this file to .env.local and fill in the values

# Resend Email Service
# Get your API key from https://resend.com/api-keys
RESEND_API_KEY=re_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# Email address to receive lead notifications
NOTIFICATION_EMAIL=info@yourdomain.com

# Supabase Database
# Get these values from your Supabase project settings
# Dashboard: https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Admin API Security
# Generate a secure random key with: openssl rand -base64 32
# This key is used to authenticate admin API endpoints
ADMIN_API_KEY=your-secure-admin-api-key-here